<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lgd.wifi_test.MainActivity">

    <EditText
        android:id="@+id/main_wifi_ssid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/main_edit_ssid" />

    <EditText
        android:id="@+id/main_wifi_password"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/main_edit_password"
        app:layout_constraintTop_toBottomOf="@+id/main_wifi_ssid" />

    <Button
        android:id="@+id/main_wifi_confim"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="jhgjhghjg"
        android:text="чбошод"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />


</android.support.constraint.ConstraintLayout>
